// https://nuxt.com/docs/guide/concepts/typescript
{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": [
    "types/**/*.d.ts",
    "components",
    "composables",
    "pages",
    "layouts",
    "plugins",
    "app/pages/app.vue",
    "app/app.vue"
  ]
}
